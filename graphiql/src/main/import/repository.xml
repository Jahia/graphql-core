<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0"
         xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <graphql-core-graphiql j:dependencies="default"
                               j:modulePriority="0"
                               j:moduleType="module"
                               j:title="GraphiQL"
                               jcr:mixinTypes="jmix:hasExternalProviderExtension"
                               jcr:primaryType="jnt:module">

            <portlets jcr:primaryType="jnt:portletFolder"/>
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <site-settings-base j:hiddenTemplate="false" jcr:primaryType="jnt:template">
                    <graphql-core j:hiddenTemplate="false"
                                  j:icon="/modules/graphql-core-graphiql/templates/files/graphql-core.png"
                                  j:view="gqlSettings"
                                  jcr:mixinTypes="jmix:useUILocale"
                                  jcr:primaryType="jnt:template">
                        <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="GraphQL"/>
                        <j:translation_de jcr:language="de"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="GraphQL"/>
                        <j:translation_fr jcr:language="fr"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="GraphQL"/>
                        <j:translation_it jcr:language="it"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="GraphQL"/>
                        <j:translation_es jcr:language="es"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="GraphQL"/>
                        <j:translation_pt jcr:language="pt"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="GraphQL"/>
                        <graphql-core-graphiql j:applyOn="jnt:virtualsite"
                                               j:defaultTemplate="false"
                                               j:hiddenTemplate="false"
                                               j:icon="/modules/graphql-core-graphiql/templates/files/graphiql.png"
                                               j:requiredPermissionNames="canSetupGraphQL"
                                               jcr:primaryType="jnt:contentTemplate">
                            <j:translation_en jcr:language="en"
                                              jcr:mixinTypes="mix:title"
                                              jcr:primaryType="jnt:translation"
                                              jcr:title="GraphiQL"/>
                            <j:translation_fr jcr:language="fr"
                                              jcr:mixinTypes="mix:title"
                                              jcr:primaryType="jnt:translation"
                                              jcr:title="GraphiQL"/>
                            <j:translation_de jcr:language="de"
                                              jcr:mixinTypes="mix:title"
                                              jcr:primaryType="jnt:translation"
                                              jcr:title="GraphiQL"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <graphiql-ui jcr:primaryType="gqlnt:graphiql"/>
                            </pagecontent>
                        </graphql-core-graphiql>
                    </graphql-core>
                </site-settings-base>
            </templates>
        </graphql-core-graphiql>
    </modules>
</content>