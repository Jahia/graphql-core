<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean class="org.jahia.test.bin.TestBean">
        <property name="testCases">
            <list>
                <value>org.jahia.test.graphql.GraphQLNodeRetrievalTest</value>
                <value>org.jahia.test.graphql.GraphQLPropertiesTest</value>
                <value>org.jahia.test.graphql.GraphQLAncestorsTest</value>
                <value>org.jahia.test.graphql.GraphQLDescendantsTest</value>
                <value>org.jahia.test.graphql.GraphQLReferencesTest</value>
                <value>org.jahia.test.graphql.GraphQLQueryTest</value>
                <value>org.jahia.test.graphql.GraphQLNodetypesTest</value>
                <value>org.jahia.test.graphql.GraphQLConnectionsTest</value>
            </list>
        </property>
